{
	"name": "sveltia-cms-auth-hono",
	"version": "0.0.1",
	"author": "MikeNewXYZ",
	"private": true,
	"scripts": {
		"format": "prettier --write .",
		"dev:package": "pnpm --filter sveltia-cms-auth-hono dev",
		"build:package": "pnpm --filter sveltia-cms-auth-hono build",
		"publish:package": "pnpm build:package && pnpm --filter sveltia-cms-auth-hono publish --no-git-checks",
		"version:package:patch": "pnpm --filter sveltia-cms-auth-hono version patch",
		"version:package:minor": "pnpm --filter sveltia-cms-auth-hono version minor",
		"version:package:major": "pnpm --filter sveltia-cms-auth-hono version major",
		"release:package:patch": "pnpm version:package:patch && pnpm publish:package",
		"release:package:minor": "pnpm version:package:minor && pnpm publish:package",
		"release:package:major": "pnpm version:package:major && pnpm publish:package"
	},
	"devDependencies": {
		"prettier": "^3.5.2",
		"prettier-plugin-astro": "^0.14.1"
	},
	"packageManager": "pnpm@9.10.0"
}
